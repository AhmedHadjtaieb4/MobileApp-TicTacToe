<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminHomePage">


    <android.support.design.widget.BottomNavigationView
        android:id="@+id/bottomNavigationViewAdmin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        app:menu="@menu/item_nav"></android.support.design.widget.BottomNavigationView>
      <FrameLayout
          android:id="@+id/frameLayoutAdmin"
          android:layout_width="match_parent"
          android:layout_height="513dp"
          android:layout_alignParentEnd="true"
          android:layout_alignParentTop="true"
          android:layout_marginEnd="0dp"
          android:layout_marginTop="0dp"
          android:layout_above="@id/bottomNavigationViewAdmin"


          ></FrameLayout>
</RelativeLayout>