        </activity>
        <activity android:name=".LogInModeSelection" />
        <activity android:name=".VerificationCode" />
        <activity android:name=".AdminHomePage" />
        <activity android:name=".MagasinierHomePage" />
        <activity android:name=".VendeurHomePage" />
    </application>

</manifest>